{
  "name": "vuex-orm-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vuex-orm/core": "0.32.3",
    "vue": "^2.5.22",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.11.0",
    "@vue/cli-plugin-eslint": "3.11.0",
    "@vue/cli-service": "3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vuex-orm",
    "vuex",
    "vue"
  ],
  "description": "A starter template for vuex-orm. Useful for bug reports, new projects and as a playground!"
}